{
    "name": "authanram/laravel-theme",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "laravel/framework": "^6.14",
        "symfony/yaml": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "Authanram\\Theme\\": "src/"
        },
        "files": [
            "./helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Authanram\\Theme\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
